{
    "name": "mir-cv",
    "authors": [
        "Relja Ljubobratovic",
        "Ilya Yaroshenko",
        "Mir Developers (see information per file)"
    ],
    "description": "Low-level (betterC) modules for dcv library.",
    "copyright": "Copyright Â© 2017, Relja Ljubobratovic, Ilya Yaroshenko",
    "license": "BSL-1.0",
    "dependencies": {
        "mir-cpuid" : "~>0.4.2",
        "mir-algorithm": "~>0.5.0"
    },
    "buildTypes": {
        "betterC": {
            "buildOptions": ["noBoundsCheck", "releaseMode", "optimize", "inline"],
            "dflags-ldc": ["-betterC", "-mcpu=native", "-enable-cross-module-inlining"]
        }
    },
    "configurations": [
        {
            "name": "default"
        },
        {
            "name": "static",
            "targetType": "staticLibrary"
        },
        {
            "name": "dynamic",
            "targetType": "dynamicLibrary"
        }
    ]
}
